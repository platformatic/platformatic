{
  "name": "@platformatic/astro",
  "version": "3.0.3",
  "description": "Platformatic Astro Capability",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "test": "node --test --test-reporter=cleaner-spec-reporter --test-concurrency=1 --test-timeout=2000000 test/*.test.js test/**/*.test.js",
    "gen-schema": "node lib/schema.js > schema.json",
    "gen-types": "json2ts > config.d.ts < schema.json",
    "build": "npm run gen-schema && npm run gen-types",
    "prepublishOnly": "npm run build",
    "lint": "eslint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/platformatic/platformatic.git"
  },
  "author": "Platformatic Inc. <oss@platformatic.dev> (https://platformatic.dev)",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/platformatic/platformatic/issues"
  },
  "homepage": "https://github.com/platformatic/platformatic#readme",
  "dependencies": {
    "@fastify/middie": "^9.0.0",
    "@fastify/static": "^8.0.0",
    "@platformatic/basic": "workspace:*",
    "@platformatic/foundation": "workspace:*",
    "fastify": "^5.0.0",
    "semver": "^7.6.3"
  },
  "devDependencies": {
    "@astrojs/node": "^9.2.0",
    "@platformatic/gateway": "workspace:*",
    "@platformatic/service": "workspace:*",
    "@types/react": "^19.1.11",
    "@types/react-dom": "^19.1.7",
    "astro": "^5.7.1",
    "cleaner-spec-reporter": "^0.5.0",
    "eslint": "9",
    "json-schema-to-typescript": "^15.0.1",
    "neostandard": "^0.12.0",
    "typescript": "^5.5.4",
    "vite": "^7.0.0",
    "ws": "^8.18.0"
  },
  "engines": {
    "node": ">=22.18.0"
  }
}
