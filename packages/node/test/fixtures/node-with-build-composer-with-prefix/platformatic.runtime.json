{
  "$schema": "https://schemas.platformatic.dev/@platformatic/runtime/2.0.0.json",
  "entrypoint": "composer",
  "server": {
    "logger": {
      "level": "trace"
    }
  },
  "services": [
    {
      "id": "frontend",
      "config": "platformatic.application.json",
      "path": "./services/frontend"
    },
    {
      "id": "backend",
      "config": "platformatic.json",
      "path": "./services/backend"
    },
    {
      "id": "composer",
      "config": "platformatic.json",
      "path": "./services/composer"
    }
  ]
}
