{
  "name": "@platformatic/sql-mapper",
  "version": "3.12.0",
  "description": "A data mapper utility for SQL databases",
  "main": "index.js",
  "type": "module",
  "types": "index.d.ts",
  "scripts": {
    "lint": "eslint",
    "test": "npm run test:typescript && npm run test:postgresql && npm run test:mariadb && npm run test:mysql && npm run test:mysql8 && npm run test:sqlite",
    "test:runner": "node --test --test-reporter=cleaner-spec-reporter --test-concurrency=1 --test-timeout=2000000 test/*.test.js test/**/*.test.js",
    "test:postgresql": "DB=postgresql npm run test:runner",
    "test:mariadb": "DB=mariadb npm run test:runner",
    "test:mysql": "DB=mysql npm run test:runner",
    "test:mysql8": "DB=mysql8 npm run test:runner",
    "test:sqlite": "DB=sqlite npm run test:runner",
    "test:typescript": "tsd"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/platformatic/platformatic.git"
  },
  "author": "Platformatic Inc. <oss@platformatic.dev> (https://platformatic.dev)",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/platformatic/platformatic/issues"
  },
  "homepage": "https://github.com/platformatic/platformatic#readme",
  "devDependencies": {
    "@opentelemetry/api": "^1.9.0",
    "cleaner-spec-reporter": "^0.5.0",
    "eslint": "9",
    "fastify": "^5.0.0",
    "neostandard": "^0.12.0",
    "tsd": "^0.33.0",
    "typescript": "^5.5.4"
  },
  "dependencies": {
    "@databases/mysql": "^7.0.0",
    "@databases/pg": "^5.5.0",
    "@databases/sql": "^3.3.0",
    "@fastify/error": "^4.0.0",
    "@hapi/topo": "^6.0.2",
    "@matteo.collina/sqlite-pool": "^0.6.0",
    "@platformatic/foundation": "workspace:*",
    "@platformatic/telemetry": "workspace:*",
    "async-cache-dedupe": "^2.1.0",
    "camelcase": "^6.3.0",
    "fastify-plugin": "^5.0.0",
    "inflected": "^2.1.0"
  },
  "tsd": {
    "directory": "test/types"
  },
  "engines": {
    "node": ">=22.19.0"
  }
}
