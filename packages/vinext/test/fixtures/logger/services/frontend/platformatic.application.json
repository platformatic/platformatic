{
  "$schema": "https://schemas.platformatic.dev/@platformatic/vinext/2.0.0.json",
  "logger": {
    "level": "debug",
    "formatters": {
      "path": "logger-formatters.js"
    },
    "timestamp": "isoTime",
    "redact": {
      "paths": [
        "req.host"
      ],
      "censor": "***HIDDEN***"
    }
  },
  "application": {
    "commands": {
      "build": "npm run build",
      "production": "npm run start"
    }
  }
}
