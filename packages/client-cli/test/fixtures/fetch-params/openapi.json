{
  "openapi": "3.0.3",
  "info": {
    "title": "Fetch Parameters Test",
    "description": "Testing custom fetch parameters",
    "version": "1.0.0"
  },
  "paths": {
    "/request-info": {
      "get": {
        "operationId": "getRequestInfo",
        "responses": {
          "200": {
            "description": "Request information",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "method": {
                      "type": "string"
                    },
                    "cache": {
                      "type": "string"
                    },
                    "mode": {
                      "type": "string"
                    }
                  }
                }
              }
            }
          }
        }
      }
    }
  }
}
