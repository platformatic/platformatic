{
  "server": {
    "hostname": "127.0.0.1",
    "port": "0",
    "logger": {
      "level": "error"
    }
  },
  "db": {
    "connectionString": "sqlite://./db.sqlite",
    "graphql": true,
    "openapi": true
  },
  "migrations": {
    "dir": "./migrations",
    "autoApply": true
  },
  "plugins": {
    "paths": [
      "./plugin.js"
    ]
  },
  "telemetry": {
    "serviceName": "test-service",
    "version": "1.0.0",
    "exporter": {
      "type": "memory"
    }
  }
}
