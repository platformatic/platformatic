{
  "$schema": "https://schemas.platformatic.dev/@platformatic/service/1.52.0.json",
  "server": {
    "hostname": "{PLT_SERVER_HOSTNAME}",
    "port": "{PORT}",
    "logger": {
      "level": "{PLT_SERVER_LOGGER_LEVEL}"
    }
  },
  "service": {
    "openapi": true
  },
  "plugins": {
    "paths": [{
      "path": "plugins",
      "encapsulate": false
    }, {
      "path": "modules",
      "encapsulate": false,
      "maxDepth": 1
    }]
  }
}
