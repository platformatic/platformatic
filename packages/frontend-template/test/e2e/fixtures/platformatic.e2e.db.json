{
  "$schema": "https://schemas.platformatic.dev/@platformatic/db/1.52.0.json",
  "server": {
    "hostname": "127.0.0.1",
    "port": "9999",
    "logger": {
      "level": "info"
    },
    "cors": {
      "origin": {
        "regexp": "/*/"
      }
    }
  },
  "db": {
    "connectionString": "sqlite://./db.sqlite",
    "graphql": true,
    "openapi": true
  },
  "migrations": {
    "dir": "migrations",
    "autoApply": true
  },
  "types": {
    "autogenerate": false
  }
}
