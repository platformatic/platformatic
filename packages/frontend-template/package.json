    "es-main": "^1.3.0",
    "execa": "^9.0.0",
    "help-me": "^5.0.0",
    "jsonpointer": "^5.0.1",
    "minimist": "^1.2.8",
    "scheduler": "^0.26.0",
    "undici": "^7.0.0",
    "wattpm": "workspace:*"
  },
  "devDependencies": {
    "@platformatic/utils": "workspace:*",
    "@playwright/test": "^1.42.1",
    "@types/react": "^19.1.6",
