{
  "name": "@platformatic/frontend-template",
  "version": "1.53.0",
  "main": "index.js",
  "type": "module",
  "description": "Platformatic Frontend Template",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/platformatic/platformatic.git"
  },
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/platformatic/platformatic/issues"
  },
  "homepage": "https://github.com/platformatic/platformatic#readme",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test:e2e": "playwright test",
    "test:types": "tsc",
    "test:server": "node ../cli/cli.js db start --config=test/e2e/fixtures/platformatic.e2e.db.json",
    "test:prepare": "node test/prepare.mjs",
    "test:cleanup": "rm -rf src/platformatic-generated-code test/e2e/fixtures/db.sqlite",
    "lint": "standard | snazzy",
    "test": "npm run lint && npm run test:e2e && npm run test:types"
  },
  "dependencies": {
    "@platformatic/client": "workspace:*",
    "@platformatic/db": "workspace:*",
    "camelcase": "^8.0.0",
    "code-block-writer": "^13.0.2",
    "desm": "^1.3.1",
    "es-main": "^1.3.0",
    "execa": "^8.0.1",
    "help-me": "^5.0.0",
    "jsonpointer": "^5.0.1",
    "minimist": "^1.2.8",
    "undici": "^6.19.8"
  },
  "devDependencies": {
    "@playwright/test": "^1.47.0",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^3.1.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "snazzy": "^9.0.0",
    "standard": "^17.1.0",
    "typescript": "^5.6.2",
    "vite": "^5.4.3"
  },
  "standard": {
    "globals": [
      "describe",
      "context",
      "before",
      "beforeEach",
      "after",
      "afterEach",
      "it",
      "expect",
      "test",
      "screen"
    ],
    "ignore": [
      "/build"
    ]
  }
}
