{
  "$schema": "https://schemas.platformatic.dev/@platformatic/runtime/2.63.0.json",
  "entrypoint": "node",
  "autoload": {
    "path": "services"
  },
  "server": {
    "hostname": "127.0.0.1",
    "port": "0"
  },
  "watch": false,
  "logger": {
    "level": "debug",
    "base": null,
    "telemetryExporter": {
      "protocol": "http",
      "url": "http://localhost:4318/v1/logs"
    }
  },
  "telemetry": {
    "enabled": true,
    "applicationName": "logger-opentelemetry",
    "version": "1.0.0"
  }
}
