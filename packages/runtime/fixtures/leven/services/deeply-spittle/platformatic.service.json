{
  "$schema": "https://platformatic.dev/schemas/v0.31.0/service",
  "server": {
    "hostname": "{PLT_SERVER_HOSTNAME}",
    "port": "{PORT}",
    "logger": {
      "level": "{PLT_SERVER_LOGGER_LEVEL}"
    }
  },
  "service": {
    "openapi": true
  },
  "plugins": {
    "paths": [
      {
        "path": "./plugins",
        "encapsulate": false,
        "options": {
          "example": "{PLT_EXAMPLE}"
        }
      },
      "./routes"
    ]
  }
}
