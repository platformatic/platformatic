{
  "$schema": "https://schemas.platformatic.dev/@platformatic/runtime/0.22.0.json",
  "entrypoint": "serviceApp",
  "hotReload": true,
  "autoload": {
    "path": "../monorepo",
    "exclude": [
      "docs",
      "composerApp"
    ],
    "mappings": {
      "serviceAppWithLogger": {
        "id": "with-logger",
        "config": "platformatic.service.json"
      },
      "serviceAppWithMultiplePlugins": {
        "id": "multi-plugin-service",
        "config": "platformatic.service.json"
      },
      "dbApp": {
        "id": "db-app",
        "config": "platformatic.db.json"
      }
    }
  },
  "server": {
    "logger": {
      "level": "trace"
    }
  }
}
