{
  "$schema": "https://schemas.platformatic.dev/@platformatic/runtime/1.22.0.json",
  "entrypoint": "serviceApp",
  "watch": true,
  "autoload": {
    "path": "../monorepo",
    "exclude": ["docs", "composerApp"],
    "mappings": {
      "serviceAppWithLogger": {
        "id": "with-logger",
