{
  "$schema": "https://schemas.platformatic.dev/@platformatic/runtime/1.52.0.json",
  "entrypoint": "composer",
  "preload": ["preload-1.js", "preload-2.js"],
  "services": [
    {
      "id": "a",
      "path": "./services/a",
      "preload": ["preload-3.js", "preload-4.js"]
    },
    {
      "id": "b",
      "path": "./services/b",
      "preload": "preload-5.js"
    },
    {
      "id": "composer",
      "path": "./services/composer"
    }
  ],
  "server": {
    "hostname": "127.0.0.1",
    "port": "{{PORT}}",
    "logger": {
      "level": "error"
    }
  }
}
