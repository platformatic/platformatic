{
  "$schema": "https://platformatic.dev/schemas/v0.17.0/db",
  "server": {
    "hostname": "{PLT_SERVER_HOSTNAME}",
    "port": "{PORT}",
    "logger": {
      "level": "{PLT_SERVER_LOGGER_LEVEL}"
    }
  },
  "core": {
    "connectionString": "{DATABASE_URL}",
    "graphql": true,
    "openapi": true,
    "events": false
  },
  "migrations": {
    "dir": "migrations"
  },
  "plugins": {
    "paths": [
      "./plugin/index.js"
    ]
  }
}
