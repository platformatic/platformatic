{
  "name": "@platformatic/sql-graphql",
  "version": "1.16.0",
  "description": "Map SQL dbs to GraphQL",
  "main": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "lint": "standard | snazzy",
    "test": "standard | snazzy && npm run test:typescript && npm run test:postgresql && npm run test:mariadb && npm run test:mysql && npm run test:mysql8 && npm run test:sqlite",
    "test:postgresql": "DB=postgresql borp --concurrency=1 test/*.test.js",
    "test:mariadb": "DB=mariadb borp --concurrency=1 test/*.test.js",
    "test:mysql": "DB=mysql borp --concurrency=1 test/*.test.js",
    "test:mysql8": "DB=mysql8 borp --concurrency=1 test/*.test.js",
    "test:sqlite": "DB=sqlite borp --concurrency=1 test/*.test.js",
    "test:typescript": "tsd"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/platformatic/platformatic.git"
  },
  "author": "Matteo Collina <hello@matteocollina.com>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/platformatic/platformatic/issues"
  },
  "homepage": "https://github.com/platformatic/platformatic#readme",
  "devDependencies": {
    "@matteo.collina/snap": "^0.1.0",
    "@matteo.collina/tspl": "^0.1.0",
    "@mercuriusjs/gateway": "^1.2.0",
    "@platformatic/sql-events": "workspace:*",
    "@platformatic/sql-mapper": "workspace:*",
    "@platformatic/telemetry": "workspace:*",
    "borp": "^0.4.2",
    "fastify": "^4.23.2",
    "snazzy": "^9.0.0",
    "standard": "^17.1.0",
    "tsd": "^0.29.0",
    "why-is-node-running": "^2.2.2",
    "ws": "^8.14.2"
  },
  "dependencies": {
    "@fastify/error": "^3.3.0",
    "@mercuriusjs/federation": "^2.0.0",
    "@platformatic/utils": "workspace:*",
    "camelcase": "^6.3.0",
    "fastify-plugin": "^4.5.1",
    "graphql": "^16.8.1",
    "graphql-scalars": "^1.22.2",
    "graphql-type-json": "^0.3.2",
    "inflected": "^2.1.0",
    "mercurius": "^13.1.0"
  },
  "tsd": {
    "directory": "test/types"
  }
}
