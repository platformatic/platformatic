{
  "$schema": "https://schemas.platformatic.dev/wattpm/2.18.0.json",
  "server": {
    "hostname": "127.0.0.1",
    "port": 3042
  },
  "logger": {
    "level": "info"
  },
  "entrypoint": "composer",
  "autoload": {
    "path": "api"
  },
  "services": [
    {
      "id": "express",
      "path": "./api/express",
      "telemetry": {
        "instrumentations": [
          "@opentelemetry/instrumentation-pg"
        ]
      }
    }
  ],
  "basePath": "/",
  "telemetry": {
    "applicationName": "test-pg",
    "version": "1.0.0",
    "exporter": {
      "type": "file"
    }
  }
}
