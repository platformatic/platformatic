    "@platformatic/utils": "workspace:*",
    "@types/node": "^20.11.15",
    "boring-name-generator": "^1.0.3",
    "chalk": "^5.3.0",
    "columnify": "^1.6.0",
    "commist": "^3.2.0",
    "desm": "^1.3.1",
    "es-main": "^1.3.0",
    "execa": "^8.0.1",
    "fastify": "^4.26.0",
    "help-me": "^5.0.0",
