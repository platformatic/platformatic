{
  "name": "@platformatic/next",
  "version": "2.70.1",
  "description": "Platformatic Next.js Stackable",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "test": "npm run lint && borp --concurrency=1 --timeout 1200000",
    "coverage": "npm run lint && borp -C -X test -X test/fixtures --concurrency=1 --timeout 1200000",
    "gen-schema": "node lib/schema.js > schema.json",
    "gen-types": "json2ts > config.d.ts < schema.json",
    "build": "pnpm run gen-schema && pnpm run gen-types",
    "prepublishOnly": "pnpm run build",
    "lint": "eslint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/platformatic/platformatic.git"
  },
  "author": "Platformatic Inc. <oss@platformatic.dev> (https://platformatic.dev)",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/platformatic/platformatic/issues"
  },
  "homepage": "https://github.com/platformatic/platformatic#readme",
  "dependencies": {
    "@babel/generator": "^7.25.0",
    "@babel/parser": "^7.25.3",
    "@babel/traverse": "^7.25.3",
    "@babel/types": "^7.25.2",
    "@platformatic/basic": "workspace:*",
    "@platformatic/utils": "workspace:*",
    "amaro": "^0.3.0",
    "iovalkey": "^0.3.0",
    "msgpackr": "^1.11.2",
    "semver": "^7.6.3"
  },
  "devDependencies": {
    "@fastify/reply-from": "^12.0.0",
    "@platformatic/composer": "workspace:*",
    "@platformatic/service": "workspace:*",
    "@types/node": "^22.5.0",
    "borp": "^0.20.0",
    "eslint": "9",
    "execa": "^9.5.1",
    "fastify": "^5.0.0",
    "json-schema-to-typescript": "^15.0.1",
    "neostandard": "^0.12.0",
    "next": "^15.0.0",
    "typescript": "^5.5.4",
    "ws": "^8.18.0"
  }
}
