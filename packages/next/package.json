    "@types/react-dom": "^19.1.7",
    "cleaner-spec-reporter": "^0.5.0",
    "eslint": "9",
    "execa": "^9.5.1",
    "fastify": "^5.0.0",
    "json-schema-to-typescript": "^15.0.1",
    "neostandard": "^0.12.0",
    "next": "^15.0.0",
    "typescript": "^5.5.4",
    "ws": "^8.18.0"
  },
  "engines": {
    "node": ">=22.18.0"
  }
